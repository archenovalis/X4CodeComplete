{
  "name": "x4codecomplete",
  "displayName": "X4CodeComplete",
  "description": "",
  "version": "1.1.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/archenovalis/X4CodeComplete"
  },
  "publisher": "CharlieGettys and Archie",
  "license": "MIT",
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:xml"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "X4 Code Complete Settings",
      "properties": {
        "x4CodeComplete.unpackedFileLocation": {
          "type": "string",
          "default": "",
          "description": "Specifies the path to your unpacked files. Required for the plugin to work."
        },
        "x4CodeComplete.exceedinglyVerbose": {
          "type": "boolean",
          "default": false,
          "description": "Spams the log with info on autocomplete options. Useful for development and debugging, and not much else. May degrade performance significantly."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "@types/xml2js": "^0.4.14",
    "minimatch": "^10.0.1",
    "minimist": "^1.2.8",
    "mkdirp": "^3.0.1",
    "mocha": "^11.0.1",
    "tslint": "^5.20.1",
    "typescript": "^5.7.2",
    "vscode": "^1.1.34"
  },
  "dependencies": {
    "xml2js": "^0.6.2",
    "xml2js-xpath": "^0.13.0"
  }
}
